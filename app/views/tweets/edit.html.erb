<%= link_to("Home", root_path) %> >
<%= link_to("Edit", edit_tweet_path(@tweet)) %>
<h1>edit tweet</h1>
<%= form_for(@tweet) do |f| %>
  <p>
    <%= f.label :status %><br/>
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :zombie_id %><br/>
    <%= f.number_field :zombie_id %>
  </p>
  <p>
    <%= f.submit %>
    <%= link_to("Show", tweet_path(@tweet)) %>
  </p>
<% end %>